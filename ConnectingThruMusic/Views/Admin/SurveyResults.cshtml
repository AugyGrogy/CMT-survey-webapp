
@model List<SurveyResults>
@{
    ViewBag.Title = "Survey Results";
}

<h1>Survey Results</h1>

<div>
    <h2>Instructor</h2>
    <table class="display" id="surveyInstructorDatatable">
        <thead>
            <tr>
                <th>
                    Instructor ID
                </th>
                <th>
                    Question ID
                </th>
                <th>
                    Question Type
                </th>
                <th>
                    Answer ID
                </th>
                <th>
                    Answer
                </th>
                <th>
                    Group ID
                </th>
                <th>
                    Quarter ID
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var results in Model)
            {
                if (@results.instructorID != 0)
                {


                    <tr>
                        <td>
                            @results.instructorID
                        </td>
                        <td>
                            @results.QuestionID
                        </td>
                        <td>
                            @results.type
                        </td>
                        <td>
                            @results.answerID
                        </td>
                        <td>
                            @results.answer
                        </td>
                        <td>
                            @results.groupID
                        </td>
                        <td>
                            @results.quarterID
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
<br />
<div>
    <h2>Parent</h2>
    <table class="display" id="surveyParentDatatable">
        <thead>
            <tr>
                <th>
                    Parent ID
                </th>
                <th>
                    Question ID
                </th>
                <th>
                    Question Type
                </th>
                <th>
                    Answer ID
                </th>
                <th>
                    Answer
                </th>
                <th>
                    Group ID
                </th>
                <th>
                    Quarter ID
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var results in Model)
            {
                if (@results.parentID != 0)
                {


                    <tr>
                        <td>
                            @results.parentID
                        </td>
                        <td>
                            @results.QuestionID
                        </td>
                        <td>
                            @results.type
                        </td>
                        <td>
                            @results.answerID
                        </td>
                        <td>
                            @results.answer
                        </td>
                        <td>
                            @results.groupID
                        </td>
                        <td>
                            @results.quarterID
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
<br />

<script src="~/js/jquery.dataTables.js" defer></script>
<link href="https://cdn.datatables.net/1.11.4/css/jquery.dataTables.min.css" rel="stylesheet" />
<script type="text/javascript" src="https://cdn.datatables.net/buttons/2.2.2/js/dataTables.buttons.min.js" defer></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" defer></script>
<script type="text/javascript" src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.html5.min.js" defer></script>
<script src="~/js/pdfmake.js" defer></script>
<script src="~/js/vfs_fonts.js" defer></script>
<script type="text/javascript" src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.print.min.js" defer></script>
<script>
    $(document).ready(function () {
        $('table.display').DataTable({
            dom: 'Bfrtip',
            buttons: ['copy', 'excel', 'pdf', 'print']
        });
    });
</script>
